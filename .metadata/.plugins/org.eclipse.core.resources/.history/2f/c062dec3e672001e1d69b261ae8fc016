package com.springboot.restfulwebservice.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;
import java.util.Date;

@Entity
@Table(name = "daftar_pasien")
public class DaftarPasien {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id_pasien;

    private String nama;

    private int usia;

    private String alamat;

    private String jenis_kelamin;

    @Column(name = "nomor_kamar")
    private int nomorKamar;

    @Column(name = "tanggal_masuk")
    private Date tanggalMasuk;

    public DaftarPasien() {
        // Default constructor
    }

    public DaftarPasien(String nama, int usia, String alamat, String jenis_kelamin, int nomorKamar, Date tanggalMasuk) {
        this.nama = nama;
        this.usia = usia;
        this.alamat = alamat;
        this.jenis_kelamin = jenis_kelamin;
        this.nomorKamar = nomorKamar;
        this.tanggalMasuk = tanggalMasuk;
    }

    public Long getId_pasien() {
        return id_pasien;
    }

    public void setId_pasien(Long id_pasien) {
        this.id_pasien = id_pasien;
    }

    public String getNama() {
        return nama;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public int getUsia() {
        return usia;
    }

    public void setUsia(int usia) {
        this.usia = usia;
    }

    public String getAlamat() {
        return alamat;
    }

    public void setAlamat(String alamat) {
        this.alamat = alamat;
    }

    public String getJenis_kelamin() {
        return jenis_kelamin;
    }

    public void setJenis_kelamin(String jenis_kelamin) {
        this.jenis_kelamin = jenis_kelamin;
    }

    public int getNomorKamar() {
        return nomorKamar;
    }

    public void setNomorKamar(int nomorKamar) {
        this.nomorKamar = nomorKamar;
    }

    public Date getTanggalMasuk() {
        return tanggalMasuk;
    }

    public void setTanggalMasuk(Date tanggalMasuk) {
        this.tanggalMasuk = tanggalMasuk;
    }
}
